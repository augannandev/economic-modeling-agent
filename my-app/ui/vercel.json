{
  "buildCommand": "cd ../.. && pnpm install && cd my-app/ui && pnpm build",
  "outputDirectory": "dist",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@backend_api_url",
    "VITE_USE_FIREBASE_EMULATOR": "false"
  }
}

